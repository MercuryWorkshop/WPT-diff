{
	"editor.defaultFormatter": "biomejs.biome",
	"notebook.defaultFormatter": "biomejs.biome",
	"[json]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"github.copilot.enable": {
		"*": false
	},
	"tabnine.codeLensEnabled": false,
	"tabnine.experimentalAutoImports": null,
	"tabnine.disableFileRegex": [".*"],
	"tabnine.disableLineRegex": [".*"],
	"cody.autocomplete.completeSuggestWidgetSelection": false,
	"cody.autocomplete.disableInsideComments": false,
	"cody.commandHints.enabled": false,
	"cody.chat.agenticContext": false,
	"cody.rules.enabled": true,
	"[typescript]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"files.exclude": {
		"**/.git": false,
		"**/.svn": true,
		"**/.hg": true,
		"**/.DS_Store": true,
		"**/Thumbs.db": true,
		"**/CVS": true,
		"**/.circleci": false,
		"**/.compodocrc*": false,
		"**/.editorconfig": false,
		"**/.gitattributes": false,
		"**/.github": false,
		"**/.gitkeep": false,
		"**/.gitignore": false,
		"**/.npmrc": false,
		"**/.prettierignore": false,
		"**/.prettierrc*": false,
		"**/.scannerwork": false,
		"**/.eslint*": false,
		"**/.docker*": false,
		"**/docker": false,
		"**/.nvmrc": false,
		"**/.vscodeignore": false,
		"**/.vscode": false,
		"**/CHANGELOG.md": false,
		"**/README.md": false,
		"**/LICENSE.md": false,
		"**/angular.json": false,
		"**/workspace.json": false,
		"**/jest-mocks.ts": false,
		"**/jest.config.js": false,
		"**/jest.preset.js": false,
		"**/nx.json": false,
		"**/package-lock.json": false,
		"**/package.json": false,
		"**/sonar-project.properties": false,
		"**/tsconfig*": false,
		"**/tslint*": false,
		"**/babel*": false,
		"**/.babel*": false,
		"**/webpack*": false,
		"**/typings.d.ts": false,
		"**/*.yaml": false,
		"**/Makefile": false,
		"**/*.sh": false
	},
	"search.exclude": {
		"**/wpt": true
	}
}
